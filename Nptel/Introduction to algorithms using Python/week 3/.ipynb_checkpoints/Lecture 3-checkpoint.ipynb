    "def findpost(l,v):\n",
    "    for i in range(len(l)):\n",
    "        if l[i]==v:\n",
    "            pos=i\n",
    "            break\n",
    "    else:\n",
    "        pos=-1\n",
    "        \n",
    "    return pos\n",
    "\n",
    "l=[34,5,3]\n",
    "v=54\n",
    "print(findpost(l,v))"
